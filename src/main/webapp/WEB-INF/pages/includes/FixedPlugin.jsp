<%@ page language="java" contentType="text/html; charset=UTF-8"
	pageEncoding="UTF-8"%>
<div class="fixed-plugin">
	<div class="dropdown">
		<a role="button" data-toggle="dropdown" aria-expanded="true"> <i
			class="fa fa-cog fa-2x"> </i>
		</a>
		<ul class="dropdown-menu">
			<li class="header-title">Cấu hình sidebar</li>
			<li class="adjustments-line"><a class="switch-trigger">
					<p>Hình nền</p>
					<div class="switch switch-sidebar-image has-switch"
						data-on-label="ON" data-off-label="OFF">
						<input type="checkbox" checked="checked">
					</div>
					<div class="clearfix"></div>
			</a></li>
			<li class="adjustments-line"><a class="switch-trigger">
					<p>Kích thước nhỏ</p>
					<div class="switch switch-sidebar-mini has-switch"
						data-on-label="ON" data-off-label="OFF">
						<input type="checkbox">
					</div>
					<div class="clearfix"></div>
			</a></li>
			<li class="adjustments-line"><a class="switch-trigger">
					<p>Phông nền</p>
					<div class="pull-right">
						<span class="badge filter active" data-color="black"></span> <span
							class="badge filter badge-azure" data-color="azure"></span> <span
							class="badge filter badge-green" data-color="green"></span> <span
							class="badge filter badge-orange" data-color="orange"></span> <span
							class="badge filter badge-red" data-color="red"></span> <span
							class="badge filter badge-purple" data-color="purple"></span>
					</div>
					<div class="clearfix"></div>
			</a></li>
			<li class="header-title">Hình ảnh</li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-0.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-1.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-2.jpg" />">
			</a></li>
			<li class="active"><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-3.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-4.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-5.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-6.jpg" />">
			</a></li>
			<li><a class="img-holder switch-trigger""> <img
					src="<c:url value="/resources/img/full-screen-image-7.jpg" />">
			</a></li>
		</ul>
	</div>
</div>